{
  "manifest_version": 2,
  "name": "Auto Bookmark Classification",
  "version": "1.0",
  "description": "Automatically classifies bookmarks based on the domain.",
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "64": "icon16.png"
    }
  },
  "icons": {
    "64": "icon16.png"
  }
}